# Application settings
APP_NAME=AlgoSuite
ENVIRONMENT=development
DEBUG=True
API_PREFIX=/api
API_V1_STR=/v1

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30
ALGORITHM=HS256

# Database
DATABASE_URL=postgresql+psycopg2://postgres:postgres@db:5432/algosuite
DATABASE_TEST_URL=postgresql+psycopg2://postgres:postgres@db:5432/test_algosuite

# Keycloak
KEYCLOAK_URL=http://keycloak:8080
KEYCLOAK_REALM=master
KEYCLOAK_CLIENT_ID=algosuite-backend
KEYCLOAK_CLIENT_SECRET=development_client_secret

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here
