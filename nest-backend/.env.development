# Database Configuration
dbHost=localhost
dbPort=5432
dbUsername=postgres
dbPassword=postgres
dbDatabase=algosuite_dev
dbSynchronize=true
dbLogging=true
dbPoolMax=20
dbPoolIdleTimeout=30000
dbPoolConnectionTimeout=2000
dbRetryAttempts=10
dbRetryDelay=3000
dbMigrationsRun=false

# Application Configuration
port=3000
nodeEnv=development
apiPrefix=api
swaggerPath=api/docs
corsOrigin=*

# JWT Configuration
jwtSecret=dev-secret-key
jwtExpiration=3600