# Database Configuration
dbHost=db.production.example.com
dbPort=5432
dbUsername=prod_user
dbPassword=prod_password
dbDatabase=algosuite_prod
dbSynchronize=false
dbLogging=false
dbPoolMax=50
dbPoolIdleTimeout=30000
dbPoolConnectionTimeout=2000
dbRetryAttempts=10
dbRetryDelay=3000
dbMigrationsRun=false

# Application Configuration
port=3000
nodeEnv=production
apiPrefix=api
swaggerPath=api/docs
corsOrigin=https://algosuite.example.com

# JWT Configuration
jwtSecret=${JWT_SECRET}
jwtExpiration=86400